	<content><![CDATA[
ll cf(ll n)
{
	ll cnt=0;
	ll sq= sqrt(n);

	if(sq*sq==n)
	cnt++;

	for(i=1;i<sqrt(n);i++)
	{
		if(n%i==0)
		cnt+=2;
	}

	return cnt;
}
]]></content>
